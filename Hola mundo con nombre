// HolaMundoconnombre.cpp : Este archivo contiene la función "main". La ejecución del programa comienza y termina ahí.
//

#include <iostream>
#include <string>

using namespace std;

int main()
{
	string nombre; // Es una variable nombre
	cout << "Ingresa tu nombre: "; 
	cin >> nombre;
	cout << "Hola Mundo!" << nombre;
	return 0;
}
